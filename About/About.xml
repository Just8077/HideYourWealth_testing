<?xml version="1.0" encoding="utf-8"?>
<ModMetaData>
  <name>Hide Your Wealth</name>
  <author>Soullcrusher, VoidNull</author>
  <packageId>vnull.hideyourwealth</packageId>
  <description>
	This mod adds a custom storage zone to RimWorld that doesn't contribute to colony wealth. 
</description>
  <modDependencies>
		<li>
			<packageId>brrainz.harmony</packageId>
			<displayName>Harmony</displayName>
			<downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
			<steamWorkshopUrl>https://steamcommunity.com/workshop/filedetails/?id=2009463077</steamWorkshopUrl>
		</li>
	</modDependencies>
	<loadAfter>
		<li>Ludeon.RimWorld</li>
		<li>Ludeon.RimWorld.Royalty</li>
		<li>Ludeon.RimWorld.Biotech</li>
		<li>Ludeon.RimWorld.Anomaly</li>
		<li>brrainz.harmony</li>
	</loadAfter>
  <supportedVersions>
    <li>1.5</li>
  </supportedVersions>
</ModMetaData>
